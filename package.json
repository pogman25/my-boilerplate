{
  "name": "react-challenge",
  "version": "1.0.2",
  "description": "Boilerplate, react, redux, webpack",
  "main": "index.js",
  "scripts": {
    "start": "set NODE_ENV=development&& webpack-dev-server --env=development --progress --host localhost --port 3000 --open",
    "build": "set NODE_ENV=production&& npm run clean && webpack --env=production --progress --profile --colors",
    "clean": "rimraf ./build/*",
    "test": "jest"
  },
  "keywords": [],
  "author": "SaPog",
  "license": "ISC",
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/jest": "^22.0.1",
    "@types/lodash": "^4.14.91",
    "@types/react": "^16.0.31",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^5.0.14",
    "@types/react-router": "^4.0.20",
    "@types/react-router-dom": "^4.2.3",
    "@types/react-transition-group": "^2.0.6",
    "@types/redux-logger": "^3.0.5",
    "awesome-typescript-loader": "^3.4.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.4.2",
    "jest": "^22.0.4",
    "node-sass": "^4.7.2",
    "postcss-cssnext": "^3.0.2",
    "postcss-csso": "^3.0.0",
    "postcss-loader": "^2.0.9",
    "postcss-reporter": "^5.0.0",
    "pug": "^2.0.0-rc.4",
    "pug-loader": "^2.3.0",
    "react-hot-loader": "^3.1.3",
    "react-transition-group": "^2.2.1",
    "redux-devtools-extension": "^2.13.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "ts-jest": "^22.0.0",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "classnames": "^2.2.5",
    "lodash": "^4.17.4",
    "moment": "^2.20.1",
    "normalize.css": "^7.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.2.1",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "setupFiles":["<rootDir>/test/utils.ts"],
    "globals": {
      "ts-jest": {
        "skipBabel": true
      }
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!src/**/*.test.{ts,tsx}",
      "!src/**/*/Loadable.{ts,tsx}",
      "!src/**/*/interfaces.ts"
    ]
  }
}
